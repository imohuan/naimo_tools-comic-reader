{
  "name": "naimo-plugin-template",
  "version": "1.0.0",
  "description": "Naimo Tools 插件开发模板",
  "type": "module",
  "scripts": {
    "init": "pnpm i && pnpm run add-electron-types && pnpm run dev",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "add-electron-types": "$env:ELECTRON_SKIP_DOWNLOAD=1; pnpm install electron --save-dev",
    "deploy": "node scripts/deploy-to-build.js"
  },
  "keywords": [
    "naimo",
    "plugin",
    "electron"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.13",
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^22.0.0",
    "electron": "^38.2.2",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.9.2",
    "vite": "^7.1.5"
  },
  "dependencies": {
    "@vitejs/plugin-vue": "^6.0.2",
    "@vueuse/core": "^14.0.0",
    "axios": "^1.13.2",
    "crypto-js": "^4.2.0",
    "mitt": "^3.0.1",
    "naive-ui": "^2.43.2",
    "pinia": "^2.3.1",
    "vue": "^3.5.24",
    "vue-router": "^4.6.3"
  }
}